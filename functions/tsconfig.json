{
  "compilerOptions": {
    "module": "commonjs",
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "outDir": "lib",
    "sourceMap": true,
    "strict": true,
    "target": "es2017",
    "skipLibCheck": true,
    "lib": ["esnext.asynciterable"]
  },
  "exclude": ["node_modules", "src/generated"],
  "compileOnSave": true,
  "include": ["src"]
}
